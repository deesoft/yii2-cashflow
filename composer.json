{
    "name": "deesoft/yii2-cashflow",
    "description": "Cashflow Application with Yii2",
    "keywords": ["yii2", "framework", "cashflow", "accounting"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/deesoft/yii2-cashflow/issues?state=open",
        "source": "https://github.com/deesoft/yii2-cashflow"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.11",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-authclient": "~2.1.0",
        "yiisoft/yii2-imagine": "~2.0",
        "yiisoft/yii2-jui": "~2.0",
        "deesoft/yii2-console": "~1.0",
        "deesoft/yii2-tools": "~1.0",
        "deesoft/yii2-queue": "~1.0",
        "mdmsoft/yii2-admin": "~2.0",
        "mdmsoft/yii2-upload-file": "~2.0",
        "mdmsoft/yii2-ar-behaviors": "~1.0",
        "mdmsoft/yii2-widgets": "~1.0",
        "firebase/php-jwt": "~2.0|~3.0|~4.0",
        "google/auth": "^0.11.0",
        "bower-asset/adminlte": "~2.3.11"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "deesoft/yii2-gii": "~1.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
